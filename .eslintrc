{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens":"off",
		"linebreak-style":"off",
		"comma-dangle":"off",
		"indent":"off",
		"no-tabs":"off",
		"eol-last":"off",
		"require-jsdoc":"off",
		"object-curly-spacing":"off"
	},
	"extends": ["eslint:recommended", "google"]
}